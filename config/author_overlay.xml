      <position x="0" y="0"/>
      <node type="text">
         <text value="Tool: "/>
         <position x="50" y="40"/>
         <alignment value="right"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
      <node type="text" name="tool">
         <text value="Object"/>
         <position x="50" y="40"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
      <node type="text">
         <text value="Pos: "/>
         <position x="50" y="22"/>
         <alignment value="right"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
      <node type="text" name="pos">
         <text value="0 0 0"/>
         <position x="50" y="22"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
      <node type="text">
         <text value="Ori: "/>
         <position x="50" y="4"/>
         <alignment value="right"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
      <node type="text" name="ori">
         <text value="0 0 0"/>
         <position x="50" y="4"/>
         <font resource="spectator font"/>
         <height value="16"/>
         <color name="text color"/>
      </node>
   </node>
   <node type="switch" name="measure switch">
      <node type="transform" name="measure layout">
         <node type="text">
           <text value="Measure Tool"/>
           <position x="-4" y="26"/>
           <alignment value="right"/>
           <font resource="spectator font"/>
           <height value="16"/>
           <color name="text color"/>
         </node>
         <node type="text" name="measure text">
           <text value=""/>
           <position x="-4" y="6"/>
           <alignment value="right"/>
           <font resource="spectator font"/>
           <height value="16"/>
           <color name="text color"/>
         </node>
      </node>
   </node>
   <layout node="measure layout">
      <x type="relative" value="1"/>
      <y type="relative" value="0"/>
   </layout>
</overlay>
</dmz>
